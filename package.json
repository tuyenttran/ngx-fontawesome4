{
    "name": "@ui4ngx/fontawesome4",
    "version": "7.1.2",
    "description": "Another Angular way to display FontAwesome (v4 only).",
    "author": "Tuyen T Tran <anhtuyen.tran@gmail.com> (www.exodussystem.com)",
    "license": "MIT",
    "main": "src/index.ts",
    "typings": "ui4ngx-fontawesome4.d.ts",
    "metadata": "ui4ngx-fontawesome4.metadata.json",
    "scripts": {
        "clean": "rimraf dist/*",
        "start": "ng serve --aot",
        "copy-scss": "copyfiles -u 1 assets/css/*.*css dist/@ui4ngx/fontawesome4",
        "remove-extras": "rimraf dist/@ui4ngx/fontawesome4/example",
        "build-packagr": "ng-packagr -p ng-package.json",
        "build-lib": "npm run clean && npm run build-packagr && npm run copy-scss && npm run remove-extras",
        "build-ghpages": "ng build --base-href \"https://tuyenttran.github.io/ngx-fontawesome4/\" --output-path \"dist-ghpages\"",
        "test": "npm run lint && ng test --watch false",
        "test-watch": "ng test",
        "e2e": "ng e2e",
        "e2e-watch": "watch \"ng e2e -s false\" src e2e --wait=1",
        "lint": "ng lint",
        "publish-local": "npm run build-lib && npm publish dist/@ui4ngx/fontawesome4/ --registry http://localhost:4873/",
        "publish-npm": "npm run build-lib && npm publish --access=public dist/@ui4ngx/fontawesome4/ --registry https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tuyenttran/ngx-fontawesome4.git"
    },
    "bugs": {
        "url": "https://github.com/tuyenttran/ngx-fontawesome4/issues"
    },
    "homepage": "https://tuyenttran.github.io/ngx-fontawesome4/",
    "keywords": [
        "@ui4ngx/fontawesome4",
        "angular-font-awesome",
        "angular-fontawesome",
        "ng-font-awesome",
        "ng-fontawesome",
        "ngx-font-awesome",
        "font-awesome",
        "fontawesome",
        "font awesome",
        "font-awesome 4",
        "fontawesome 4",
        "font awesome 4",
        "angular",
        "@angular",
        "angular 7",
        "angular 8",
        "angular 9",
        "angular 10",
        "angular 11",
        "angular 12",
        "angular 13",
        "ng",
        "ngx",
        "ui",
        "@ngx",
        "@ui4ngx"
    ],
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "peerDependencies": {
        "@angular/common": "^6.0.0 || ^7.0.0",
        "@angular/core": "^6.0.0 || ^7.0.0",
        "font-awesome": "^4.7.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.10.7",
        "@angular-devkit/core": "~7.0.1",
        "@angular/animations": "~7.0.0",
        "@angular/cli": "~7.0.1",
        "@angular/common": "~7.0.0",
        "@angular/compiler": "~7.0.0",
        "@angular/compiler-cli": "~7.0.0",
        "@angular/core": "~7.0.0",
        "@angular/forms": "~7.0.0",
        "@angular/http": "~7.0.0",
        "@angular/language-service": "~7.0.0",
        "@angular/platform-browser": "~7.0.0",
        "@angular/platform-browser-dynamic": "~7.0.0",
        "@angular/router": "~7.0.0",
        "@types/fs-extra": "^5.0.4",
        "@types/jasmine": "~2.8.8",
        "@types/node": "~10.9.4",
        "codelyzer": "~4.5.0",
        "copyfiles": "2.2.0",
        "core-js": "~2.6",
        "font-awesome": "^4.7.0",
        "font-awesome-animation": "^1.1.1",
        "fs-extra": "^7.0.0",
        "jasmine-core": "3.2.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "1.3.1",
        "ng-packagr": "4.3.1",
        "ngx-highlight-js": "^2.1.1",
        "protractor": "~5.1.2",
        "rimraf": "^2.6.1",
        "rxjs": "~6.4",
        "ts-node": "~7.0.1",
        "tsickle": "0.34.0",
        "tslib": "^1.9.0",
        "tslint": "~5.11.0",
        "typescript": "~3.1.1",
        "uglify-js": "^3.3.18",
        "watch": "^1.0.2",
        "zone.js": "^0.8.14"
    }
}
