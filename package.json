{
    "name": "@ui4ngx/fontawesome4",
    "version": "11.1.2",
    "description": "Another Angular way to display FontAwesome (v4 only).",
    "author": "Tuyen T Tran <anhtuyen.tran@gmail.com> (www.exodussystem.com)",
    "license": "MIT",
    "main": "bundles/ui4ngx-fontawesome4.umd.js",
    "typings": "ui4ngx-fontawesome4.d.ts",
    "metadata": "ui4ngx-fontawesome4.metadata.json",
    "style": "css/ngx-fontawesome.scss",
    "scripts": {
        "clean": "rimraf dist/*",
        "start": "ng serve --aot",
        "copy-scss": "copyfiles -u 1 assets/css/*.*css dist/@ui4ngx/fontawesome4",
        "remove-extras": "rimraf dist/@ui4ngx/fontawesome4/example",
        "build-packagr": "ng-packagr -p ng-package.json",
        "build-lib": "npm run clean && npm run build-packagr && npm run copy-scss && npm run remove-extras",
        "build-ghpages": "ng build --base-href \"https://tuyenttran.github.io/ngx-fontawesome4/\" --output-path \"dist-ghpages\"",
        "test": "npm run lint && ng test --watch false",
        "test-watch": "ng test",
        "e2e": "ng e2e",
        "e2e-watch": "watch \"ng e2e -s false\" src e2e --wait=1",
        "lint": "ng lint",
        "publish-local": "npm run build-lib && npm publish dist/@ui4ngx/fontawesome4/ --registry http://localhost:4873/",
        "publish-npm": "npm run build-lib && npm publish --access=public dist/@ui4ngx/fontawesome4/ --registry https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tuyenttran/ngx-fontawesome4.git"
    },
    "bugs": {
        "url": "https://github.com/tuyenttran/ngx-fontawesome4/issues"
    },
    "homepage": "https://tuyenttran.github.io/ngx-fontawesome4/",
    "keywords": [
        "@ui4ngx/fontawesome4",
        "angular-font-awesome",
        "angular-fontawesome",
        "ng-font-awesome",
        "ng-fontawesome",
        "ngx-font-awesome",
        "font-awesome",
        "fontawesome",
        "font awesome",
        "font-awesome 4",
        "fontawesome 4",
        "font awesome 4",
        "angular",
        "@angular",
        "angular 11",
        "angular 12",
        "angular 13",
        "ng",
        "ngx",
        "ui",
        "@ngx",
        "@ui4ngx"
    ],
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "peerDependencies": {
        "@angular/common": "^11.0.0",
        "@angular/core": "^11.0.0",
        "font-awesome": "^4.7.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1100.0",
        "@angular-devkit/core": "~11.0.0",
        "@angular/animations": "~11.0.0",
        "@angular/cli": "~11.0.0",
        "@angular/common": "~11.0.0",
        "@angular/compiler": "~11.0.0",
        "@angular/compiler-cli": "~11.0.0",
        "@angular/core": "~11.0.0",
        "@angular/forms": "~11.0.0",
        "@angular/language-service": "~11.0.0",
        "@angular/platform-browser": "~11.0.0",
        "@angular/platform-browser-dynamic": "~11.0.0",
        "@angular/router": "~11.0.0",
        "@types/jasmine": "~3.6.0",
        "@types/node": "~13.9.5",
        "codelyzer": "^6.0.1",
        "copyfiles": "2.2.0",
        "core-js": "^3.3.2",
        "font-awesome": "^4.7.0",
        "font-awesome-animation": "^1.1.1",
        "fs-extra": "^7.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.5.0",
        "ng-packagr": "^11.0.1",
        "ngx-highlight-js": "^11.0.0",
        "protractor": "~7.0.0",
        "rimraf": "^2.6.2",
        "rxjs": "^6.6.0",
        "ts-node": "~8.10.2",
        "tslint": "~6.1.2",
        "typescript": "~4.0.5",
        "uglify-js": "^3.9.4",
        "watch": "^1.0.2",
        "zone.js": "~0.10.3"
    }
}
